;\n      })\n      .catch((err) => {\n        setError(err.message);\n      });\n    \n    });\n\n  }, []); //keeps hook to only once, intresting?\n\n  const handleChange = (ev) => {\n    if (ev.error) {\n      setError(ev.error.message);\n    } else {\n      setError(null);\n    }\n  }\n\n  const handleSubmit = async (ev) => {\n    ev.preventDefault();\n    setProcessing(true);\n    \n\n    const payload = await stripe.confirmCardPayment(clientSecret, {\n      payment_method: {\n        card: elements.getElement(CardElement),\n        billing_details: {\n          name: ev.target.name.value,\n        },\n      },\n    });\n\n    if (payload.error) {\n      setError(`Payment failed: ${payload.error.message}`);\n      setProcessing(false);\n      console.log(\"[error]\", payload.error);\n    } else {\n      setError(null);\n      setSucceeded(true);\n      setProcessing(false);\n      setMetadata(payload.paymentIntent);\n      console.log(\"[PaymentIntent]\", payload.paymentIntent);\n    }\n\n  };\n\n  return (\n    <main className='checkout-page'>\n      <form className='checkout-form' onSubmit={handleSubmit}>\n\n          <div className='form-section'>\n            <h2>Shipping</h2>\n\n            <label>Recipient Name</label>\n            <input />\n\n            <label>Street Adress</label>\n            <input />\n\n            <label>City</label>\n            <input />\n\n            <div className='double-input'>\n              <label>State</label>\n              <input />\n\n              <label>Zip Code</label>\n              <input />\n            </div>\n            \n\n            <label>Country</label>\n            <input />\n\n          </div>\n\n          <div className='form-section'>\n            <h2>Payment</h2>\n            <label htmlFor='card-element'>Credit or Debit Card</label>\n            <CardElement \n              id='card-element' \n              options={CARD_ELEMENT_OPTIONS} \n              onChange={handleChange}/>\n            <div className='card-errors' role='alert'>{error}</div>\n          </div>\n\n          <button className='button checkout-button' type='submit'>Submit Payment</button>\n\n      </form>\n    </main>\n    \n  );\n}\n\nconst stripePromise = loadStripe('pk_test_51I0CsSIzd0xoN2NEasujFfk8OIEYrwO5KrdT4fYAjUiQ8iJVP1fumZ3XGDf1XJq0Q3C9I0wlsQxbtYY2xRAvP2eZ0024wl5yqd');\nconst CheckoutPage = () => {\n  return (\n    <Elements stripe={stripePromise}>\n      <CheckoutForm />\n    </Elements>\n  );\n}\n\nexport default CheckoutPage; \n\n\n/* 4242 4242 4242 4242 */","/Users/mica/Projects/HYFIX/Website/frontend/src/components/store/Buttons.js",[],"/Users/mica/Projects/HYFIX/Website/frontend/src/components/store/Category.js",[],"/Users/mica/Projects/HYFIX/Website/frontend/src/components/admin/add.js",[],"/Users/mica/Projects/HYFIX/Website/frontend/src/components/store/Card.js",[],"/Users/mica/Projects/HYFIX/Website/frontend/src/api.js",[],"/Users/mica/Projects/HYFIX/Website/frontend/src/components/store/Filter.js",[],{"ruleId":"86","replacedBy":"87"},{"ruleId":"88","replacedBy":"89"},{"ruleId":"90","severity":1,"message":"91","line":41,"column":10,"nodeType":"92","messageId":"93","endLine":41,"endColumn":18},{"ruleId":"90","severity":1,"message":"94","line":42,"column":10,"nodeType":"92","messageId":"93","endLine":42,"endColumn":19},{"ruleId":"90","severity":1,"message":"95","line":43,"column":10,"nodeType":"92","messageId":"93","endLine":43,"endColumn":20},{"ruleId":"96","severity":1,"message":"97","line":60,"column":6,"nodeType":"98","endLine":60,"endColumn":8,"suggestions":"99"},"no-native-reassign",["100"],"no-negated-in-lhs",["101"],"no-unused-vars","'metadata' is assigned a value but never used.","Identifier","unusedVar","'succeeded' is assigned a value but never used.","'processing' is assigned a value but never used.","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'cookies.userCart'. Either include it or remove the dependency array.","ArrayExpression",[